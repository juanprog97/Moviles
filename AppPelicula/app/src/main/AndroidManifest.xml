            android:supportsRtl="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Controladores.listMovieActivity">
        </activity>
        <activity android:name=".Controladores.registerUserActivity">
        </activity>
        <activity android:name=".Controladores.registeUser">
